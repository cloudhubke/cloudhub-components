{
  "name": "cloudhub-react-components",
  "version": "0.0.0-development",
  "description": "Various components to use in react projects",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w --require babel-core/register",
    "test:single": "mocha src/index.test.js --require babel-core/register",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel  --source-maps --copy-files --out-dir dist --ignore *.test.js src",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaithoben/cloudhub-react-components.git"
  },
  "keywords": [
    "cloudhub"
  ],
  "author": "Bernard Gaitho <gaithoben@gmail.com> (http://github.com/gaithoben)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaithoben/cloudhub-react-components/issues"
  },
  "homepage": "https://github.com/gaithoben/cloudhub-react-components#readme",
  "dependencies": {
    "@devexpress/dx-react-core": "^1.4.0",
    "@devexpress/dx-react-grid": "^1.4.0",
    "@devexpress/dx-react-grid-material-ui": "^1.4.0",
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "antd": "^3.6.5",
    "awesome-phonenumber": "^2.2.3",
    "axios": "^0.17.1",
    "lodash": "4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-intl-tel-input": "^4.3.0",
    "react-quill": "^1.2.3",
    "react-select": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/node": "7.0.0-beta.42",
    "@babel/plugin-transform-object-assign": "7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-flow": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.42",
    "@babel/preset-stage-1": "7.0.0-beta.42",
    "@babel/register": "7.0.0-beta.42",
    "babel-core": "7.0.0-bridge.0",
    "chai": "4.1.2",
    "css-loader": "0.28.8",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "ghooks": "2.0.2",
    "json-loader": "0.5.7",
    "mocha": "4.0.1",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "raw-loader": "0.5.1",
    "regenerator-runtime": "0.11.1",
    "rimraf": "2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "sass-loader": "6.0.6",
    "semantic-release": "^15.8.0",
    "style-loader": "0.19.1",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0"
  },
  "peerDependencies": {
    "@devexpress/dx-react-core": "1.4.0",
    "@devexpress/dx-react-grid": "1.4.0",
    "@devexpress/dx-react-grid-material-ui": "1.4.0",
    "@material-ui/core": ">=1.4.0",
    "@material-ui/icons": ">=1.1.0",
    "antd": ">=3.6.5",
    "awesome-phonenumber": "2.2.3",
    "axios": ">=0.17.1",
    "react": ">=16.4.1",
    "react-dom": ">=16.4.1",
    "react-intl-tel-input": ">=4.3.0",
    "react-quill": ">=1.2.3",
    "react-select": ">=1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      [
        "@babel/preset-stage-1",
        {
          "loose": true
        }
      ],
      "@babel/preset-react",
      "@babel/flow"
    ],
    "plugins": [
      "@babel/plugin-transform-object-assign"
    ]
  }
}
